{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "bumbummen99/shoppingcart": "^4.2",
        "caseyamcl/toc": "^3.0",
        "ckfinder/ckfinder-laravel-package": "^3.5",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "laravel/framework": "^7.29",
        "laravel/socialite": "^4.4",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^2.4",
        "laravelium/sitemap": "7.0.*",
        "phpfastcache/phpfastcache": "^7.1",
        "unisharp/laravel-filemanager": "^2.4",
        "omnipay/momo" : "@dev",
        "bigz/bigzvnpay" : "@dev"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Laravel\\Socialite\\SocialiteServiceProvider"
            ],
            "aliases": {
                "Socialite": "Laravel\\Socialite\\Facades\\Socialite"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "config/constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Omnipay\\Momo\\": "packages\/omnipay\/momo\/src/",
            "Bigzvnpay\\": "packages\/bigzvnpay\/src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "pay-momo": {
            "type": "path",
            "url": "./packages/omnipay/momo",
            "options": {
                "symlink": true
            }
        },
        "bigzvnpay": {
            "type": "path",
            "url": "./packages/bigzvnpay",
            "options": {
                "symlink": true
            }
        }
    }
}
